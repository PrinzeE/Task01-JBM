     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Enter a string: Prince Kumar\n",
      "No of vowels are 4\n"
     ]
    }
   ],
   "source": [
    "text = input('Enter a string: ')\n",
    "vowel = 0\n",
    "\n",
    "for ch in text:\n",
    "    if ch in 'aeiouAEIOU':\n",
    "        vowel += 1\n",
    "\n",
    "print('No of vowels are', vowel)"
